
<%=link_to("<<Back to List",{:action=>'index'})%>
<div>
  <h2>Pages Info</h2>
  <table border=1 summary="page detail view">

    <tr>
      <tr>
        <th>Subject ID</th>
        <td><%= @page.subject_id %></td>
      </tr>
    <tr>
      <th>Name</th>
      <td><%= @page.name %></td>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @page.position %></td>
    </tr>
    <tr>
      <th>Visble</th>
      <td><%= @page.visible ? 'YES' : 'NO' %></td>
    </tr>
    <tr>
      <th>Permalink</th>
      <td><%= @page.permalink%></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @page.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @page.updated_at %></td>
    </tr>
  </table>
</div>
